package ch6;

import java.util.Arrays;

public class ch604 {


	public static void main(String[] args) {
	ch604 ch = new ch604();
	System.out.println(Arrays.toString(ch.luckynumber()));
	}
	
//----------上面為執行方法的測試-------------
	public int[] luckynumber()
	{
		int[] l = new int[6];
		for (int i =0;i<6;i++)
		l[i]=(int)(Math.random()*42)+1;
		return l;
	}
}
