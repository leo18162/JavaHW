package ch5;

public class ch5041 {

	public static void main(String[] args) {
		String[] ca = {"snoopy","micky","lucky","kitty","derby"};
		int sum=0;
	
		for(int i =0;i<ca.length;i++)
		{
			for(int j=0;j<ca[i].length();j++)
			{
				switch( ca[i].charAt(j))
				{
				case 'a':
				case 'e':
				case 'i':
				case 'o':
				case 'u':
					sum++;
					break;
				}
			}
		}
		System.out.println("共有"+ sum+"個");
	}
}
