package ch6;

public class ch601 {


	public static void main(String[] args) {
	ch601 ch = new ch601();
	ch.starSquare(3, 5);
	}
	
//----------上面為執行方法的測試-------------

	public void starSquare(int width,int height)
	{
		for(int j =1 ;j<=height;j++)
		{
			for(int i =1 ;i <=width;i++)
				System.out.print("*");
			System.out.println();
		}
	}
}
