package ch5;

public class ch501 {

	public static void main(String[] args) {
		double[] a= {84.0,94.0,79.9,90.5,82.4,65.7,72.8,99.0,85.2,100.0};
		double sum=0.0,avg=0.0;
		for (int i=0 ;i<a.length;i++)
		{
			sum+=a[i];
		}
		avg =sum/a.length;
		for (int i=0;i<a.length;i++)
		{
			if(a[i]>avg)
				System.out.println("第"+i+"個"+a[i]);
		}
	}
}
